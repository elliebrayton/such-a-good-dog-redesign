{
  "name": "such-a-good-dog-redesign",
  "version": "1.0.0",
  "description": "Such A Good Dog Redesign",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/main.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss --useautoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css ",
    "compress:css": "node-sass css/style.prefix.css css/main.min.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.3.5",
    "concat": "^1.0.3",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {
    "gmap3": "^7.2.0",
    "mixitup": "^3.3.1"
  }
}
